   <bottom>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <children>
        <TextArea fx:id="logArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0" wrapText="true" VBox.vgrow="ALWAYS" />
            <FlowPane alignment="TOP_CENTER" columnHalignment="CENTER">
               <children>
              <Button fx:id="stopSimulationButton" mnemonicParsing="false" onAction="#stopSimulationAction" text="%stopButton">
                     <FlowPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </FlowPane.margin>
                  </Button>
               </children>
            </FlowPane>
         </children>
      </VBox>
   </bottom>
   <padding>
      <Insets left="5.0" right="5.0" top="5.0" />
